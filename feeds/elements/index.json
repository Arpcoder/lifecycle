---
layout: none
---
{% assign elements = site.elements | sort: "name" %}
[
{% for element in elements %}
{
"name": "{{ element.name }}",
"version": "{{ element.version }}",
"description": "{{ element.description[0] | escape }}",
"links": {{ element.links | jsonify }},
"video": "{{ element.video | escape }}",
"screenshots": {{ element.screenshots | jsonify }},
"content": "{{ element.content | strip_newlines | escape }}"
}
{% unless forloop.last %},{% endunless %}
{% endfor %}
]