---
layout: none
---
{% assign blueprints = site.blueprints | sort: "name" %}
[
{% for blueprint in blueprints %}
{
"name": "{{ blueprint.name }}",
"version": "{{ blueprint.version }}",
"description": "{{ blueprint.description[0] | escape }}",
"conclusion": "{{ blueprint.conclusion | escape }}",
"image": "{{ blueprint.image | escape }}",
"tags": {{ blueprint.tags | jsonify }},
"stage": "{{ blueprint.stage | escape }}",
"type": "{{ blueprint.type | escape }}",
"order": {{ blueprint.order }},
"status": "{{ blueprint.status | escape }}",
"maturity": "{{ blueprint.maturity | escape }}",
"version": "{{ blueprint.version }}",
"areas": {{ blueprint.areas | jsonify }},
"discussion": "{{ blueprint.discussion | escape }}",
"yaml": "{{ blueprint.yaml | escape }}",
"roles": {{ blueprint.roles | jsonify }},
"content": "{{ blueprint.content | strip_newlines | escape }}"
}
{% unless forloop.last %},{% endunless %}
{% endfor %}
]